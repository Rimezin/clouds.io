{
	"masterPicture": "public/drizzler512.png",
	"iconsPath": "/",
	"design": {
		"ios": {
			"pictureAspect": "backgroundAndMargin",
			"backgroundColor": "#1c2025",
			"margin": "14%",
			"assets": {
				"ios6AndPriorIcons": true,
				"ios7AndLaterIcons": true,
				"precomposedIcons": true,
				"declareOnlyDefaultIcon": true
			},
			"appName": "Drizzlr"
		},
		"desktopBrowser": {
			"design": "background",
			"backgroundColor": "#1c2025",
			"backgroundRadius": 0.45,
			"imageScale": 1
		},
		"windows": {
			"pictureAspect": "noChange",
			"backgroundColor": "#1c2025",
			"onConflict": "override",
			"assets": {
				"windows80Ie10Tile": false,
				"windows10Ie11EdgeTiles": {
					"small": true,
					"medium": true,
					"big": true,
					"rectangle": true
				}
			},
			"appName": "Drizzlr"
		},
		"androidChrome": {
			"pictureAspect": "backgroundAndMargin",
			"margin": "17%",
			"backgroundColor": "#1c2025",
			"themeColor": "#1c2025",
			"manifest": {
				"name": "Drizzlr",
				"startUrl": "https://drizzlr.app/",
				"display": "standalone",
				"orientation": "notSet",
				"onConflict": "override",
				"declared": true
			},
			"assets": {
				"legacyIcon": false,
				"lowResolutionIcons": false
			}
		},
		"safariPinnedTab": {
			"pictureAspect": "silhouette",
			"themeColor": "#1c2025"
		}
	},
	"settings": {
		"compression": 2,
		"scalingAlgorithm": "Mitchell",
		"errorOnImageTooSmall": false,
		"readmeFile": false,
		"htmlCodeFile": false,
		"usePathAsIs": false
	}
}